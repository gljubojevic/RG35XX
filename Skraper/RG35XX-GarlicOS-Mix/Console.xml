<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Information ShortName="RG35XX GarlicOS Console" LongName="RG35XX GarlicOS Console systems" Description="RG35XX GarlicOS media for console platforms" Author="gljubojevic"/>
	<Viewport Color="#ff000000" Width="640" Height="480"/>
	<Drawings>
		<Item Type="Screenshot" IfOrientation="IsLandscape">
			<Display X="50%" Y="50%" Width="110%" Height="110%" KeepRatio="true" Anchor="VCenterHCenter" Antialiasing="None" Transparency="0.0"/>
			<Fallback Type="ScreenshotTitle">
				<Fallback Type="LocalFile" LocalFilePath="NoScreenshot.png" />
			</Fallback>
		</Item>
		<Item Type="Screenshot" IfOrientation="IsPortrait">
			<Display X="50%" Y="50%" Width="130%" Height="130%" KeepRatio="true" Anchor="VCenterHCenter" Antialiasing="None" Transparency="0.0"/>
			<Rotation XOffset="50%" YOffset="50%" Angle="90.0" />
			<!--
			<Fallback Type="ScreenshotTitle">
				<Fallback Type="LocalFile" LocalFilePath="NoScreenshot.png" />
			</Fallback>
			-->
		</Item>
		<Item Type="Box3D">
			<Display X="98%" Y="90%" Width="30%" Height="40%" KeepRatio="true" Anchor="BottomRight" Antialiasing="Medium"/>
			<Fallback Type="Support" />
		</Item>
		<Item Type="Wheel">
			<Display X="50%" Y="0%" Width="40%" Height="30%" Anchor="TopHCenter" />
		</Item>
		<!-- System Photo on bottom left -->
		<Item Type="SystemPhoto">
			<Display X="2%" Y="98%" Width="35%" Height="35%" Anchor="BottomLeft" />
			<Fallback Type="SystemPicture">
				<Fallback Type="SystemWheel" />
			</Fallback>
		</Item>
	</Drawings>
</ImageComposition>