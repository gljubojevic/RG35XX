<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Information ShortName="RG35XX Koriki Default" LongName="RG35XX Koriki Default system" Description="RG35XX Koriki media for all emulation platforms" Author="gljubojevic"/>
	<Viewport Color="#ff000000" Width="348" Height="291"/>
	<Drawings>
		<Item Type="Screenshot">
			<Display X="100%" Y="50%" Width="1000%" Height="100%" KeepRatio="true" Anchor="VCenterRight" Antialiasing="None" Transparency="0.0"/>
			<Fallback Type="ScreenshotTitle">
				<Fallback Type="LocalFile" LocalFilePath="NoScreenshot.png" />
			</Fallback>
		</Item>
		<Item Type="Box3D">
			<Display X="98%" Y="98%" Width="30%" Height="40%" KeepRatio="true" Anchor="BottomRight" Antialiasing="Medium"/>
		</Item>
		<Item Type="Wheel">
			<Display X="50%" Y="0%" Width="40%" Height="30%" Anchor="TopHCenter" />
		</Item>
		<!-- SystemPhoto on bottom left -->
		<Item Type="SystemPhoto">
			<Display X="2%" Y="98%" Width="35%" Height="40%" Anchor="BottomLeft" />
			<Fallback Type="SystemPicture">
				<Fallback Type="SystemWheel">
					<Fallback Type="Support" />
				</Fallback>
			</Fallback>
		</Item>
	</Drawings>
</ImageComposition>