<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Information ShortName="RG35XX GarlicOS Default" LongName="RG35XX GarlicOS Default system" Description="RG35XX GarlicOS media for all emulation platforms" Author="gljubojevic"/>
	<Viewport Color="#ff000000" Width="640" Height="480"/>
	<Drawings>
		<Item Type="Screenshot">
			<Display X="50%" Y="50%" Width="110%" Height="110%" KeepRatio="true" Anchor="VCenterHCenter" Antialiasing="None" />
			<Fallback Type="ScreenshotTitle">
				<Fallback Type="LocalFile" LocalFilePath="NoScreenshot.png" />
			</Fallback>
		</Item>
		<Item Type="Box3D">
			<Display X="98%" Y="90%" Width="30%" Height="40%" KeepRatio="true" Anchor="BottomRight" Antialiasing="Medium"/>
			<Fallback Type="Support" />
		</Item>
		<Item Type="Wheel">
			<Display X="50%" Y="0%" Width="40%" Height="30%" Anchor="TopHCenter" />
		</Item>
		<!-- System Wheel on bottom left -->
		<Item Type="SystemWheel">
			<Display X="0%" Y="90%" Width="40%" Height="40%" Anchor="BottomLeft" />
		</Item>
	</Drawings>
</ImageComposition>